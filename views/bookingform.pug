extends layout

block content
  h1= title
  p Welcome to #{title}
  each vehicle in vehicles
      div.container
        form
          .form-group
            label(for='name') Name
            input.form-control(type='text')
          .form-group
            label(for='date') Date Selected
            input.form-control(type='text')
          .form-group
            label(for='hour') Time Selected
            input.form-control(type='text')
          .form-group
            label(for='model') Select a car
            select.custom-select
              option(value='#{vehicle.model}') #{vehicle.model}


  

    


    


